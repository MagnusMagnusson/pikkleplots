
function random_name(_id){
	var is_female = _id % 2 == 0;
	var has_suffix = (_id >> 1) % 3 == 1;
	var has_prefix = (_id >> 2) % 3 == 1;
	
	var prefix = has_prefix ? (is_female ? random_female_prefix(_id)+" " : random_male_prefix(_id)+" ") : "";
	var suffix = has_suffix ? (is_female ? " "+random_female_suffix(_id) : " "+random_male_suffix(_id)) : "";
	var name =is_female ? random_female_name(_id) : random_male_name(_id);
	
	return string("{0}{1}{2}", prefix, name, suffix);
}

